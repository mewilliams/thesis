	Keywords = {nearshore dispersion, Numerical modeling, river plume, wave-current interactions},
	Language = {en},
	Pages = {n/a--n/a},
	Title = {Effects of waves on coastal water dispersion in a small estuarine bay},
	Url = {http://onlinelibrary.wiley.com/doi/10.1002/2013JC009466/abstract},
	Urldate = {2013-12-17},
	Year = {2013},
