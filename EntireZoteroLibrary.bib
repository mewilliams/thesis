	Language = {en},
	Month = mar,
	Number = {3},
	Pages = {182--186},
	Title = {Rapid circulation of warm subtropical waters in a major glacial fjord in East Greenland},
	Url = {http://www.nature.com/ngeo/journal/v3/n3/abs/ngeo764.html},
	Urldate = {2014-01-06},
	Volume = {3},
	Year = {2010},
