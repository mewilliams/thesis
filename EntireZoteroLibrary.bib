	Pages = {2775--2786},
	Title = {Point sink flow in a linearly stratified fluid of finite depth},
	Url = {http://pof.aip.org/resource/1/phfle6/v12/i11/p2775_s1},
	Urldate = {2012-09-26},
	Volume = {12},
