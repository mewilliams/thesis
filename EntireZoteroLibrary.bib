	Journal = {Estuarine, Coastal and Shelf Science},
	Keywords = {estuary mouth closure, fluvial discharge, marine intrusion, microtidal estuaries, physico-chemical characteristics, Western Australia, south coast, 116.36$\,^{\circ}$-121.19$\,^{\circ}$ E, 33.80$\,^{\circ}$-35.04$\,^{\circ}$ S},
