	Issn = {0169-555X},
	Journal = {Geomorphology},
	Keywords = {1985 Mexico earthquake, Multi-proxy approach, Sediment record, Storm, Tsunami, Tsunami hazard},
	Month = feb,
	Pages = {360--371},
