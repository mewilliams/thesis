	Shorttitle = {Holocene geologic history of a transform margin estuary},
	Title = {Holocene geologic history of a transform margin estuary: Elkhorn Slough, central California},
	Url = {http://www.sciencedirect.com/science/article/pii/0272771486900442},
	Urldate = {2012-02-22},
	Volume = {22},
