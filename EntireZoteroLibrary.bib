	Issn = {0921-030X, 1573-0840},
	Journal = {Nat Hazards},
	Keywords = {Cascadia Subduction Zone, Cascadia tsunami danger, Civil Engineering, Environmental Management, Geophysics/Geodesy, Geotechnical Engineering, Hydrogeology, Numerical modeling, Puget Sound, shallow water equations, tsunami models, tsunami source},
	Language = {en},
