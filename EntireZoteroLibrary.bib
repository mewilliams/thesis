	Issn = {2156-2202},
	Journal = {Journal of Geophysical Research: Oceans},
	Language = {en},
	Number = {C4},
	Pages = {7649--7666},
	Shorttitle = {A third-generation wave model for coastal regions},
	Title = {A third-generation wave model for coastal regions: 1. Model description and validation},
	Url = {http://onlinelibrary.wiley.com/doi/10.1029/98JC02622/abstract},
	Urldate = {2013-01-31},
