	Doi = {10.1175/2009JPO4142.1},
	File = {EBSCO Full Text:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/PBGSDMJD/Burchard - 2009 - Combined Effects of Wind, Tide, and Horizontal Den.pdf:application/pdf},
	Issn = {00223670},
