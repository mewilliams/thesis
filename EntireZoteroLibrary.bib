	Issn = {0278-4343},
	Journal = {Continental Shelf Research},
	Keywords = {Restored wetland, Tidal inlet stability, Tidal marsh, Ultra refracted swell waves},
	Month = oct,
	Number = {16},
	Pages = {1682--1687},
	Title = {Waves and tides responsible for the intermittent closure of the entrance of a small, sheltered tidal wetland at San Francisco, {CA}},
