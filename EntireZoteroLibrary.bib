	Keywords = {Hydrodynamic, {MOHID}, {\'O}bidos Lagoon, Tidal inlet morphology, Wave radiation stresses, Wave set-up},
	Month = may,
	Number = {9},
	Pages = {1240--1250},
	Title = {Effect of coastal waves on sea level in {\'O}bidos Lagoon, Portugal},
