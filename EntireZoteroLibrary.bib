	Doi = {10.1016/j.csr.2007.12.007},
	File = {ScienceDirect Full Text PDF:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/A4U98HMH/Kularatne and Pattiaratchi - 2008 - Turbulent kinetic energy and sediment resuspension.pdf:application/pdf;ScienceDirect Snapshot:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/B23WEE59/S0278434307003317.html:text/html},
	Issn = {0278-4343},
