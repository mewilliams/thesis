	Issn = {0278-4343},
	Journal = {Continental Shelf Research},
	Keywords = {climate change, Morphodynamic modeling, Morphological evolution, wave climate, Wave-dominated tidal inlet},
	Month = nov,
	Number = {18},
	Pages = {1915--1930},
	Title = {Future evolution of a tidal inlet due to changes in wave climate, Sea level and lagoon morphology ({\'O}bidos lagoon, Portugal)},
	Url = {http://www.sciencedirect.com/science/article/pii/S0278434311002962},
	Urldate = {2013-07-24},
