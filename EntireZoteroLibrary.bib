	Journal = {Journal of Coastal Research},
	Month = aug,
	Note = {{ArticleType}: research-article / Issue Title: Special Issue 34. International Coastal Symposium ({ICS} 2000): {CHALLENGES} {FOR} {THE} 21ST {CENTURY} {IN} {COASTAL} {SCIENCES}, {ENGINEERING} {AND} {ENVIRONMENT} / Full publication date: August 2001 / Copyright {\copyright} 2001 Coastal Education \& Research Foundation, Inc.},
