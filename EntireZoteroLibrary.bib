	Issn = {0169-555X},
	Journal = {Geomorphology},
	Keywords = {Australia, beach berms, beach morphology, Closure, hydrodynamics, {ICOLL}, intermittently closed estuaries, natural beach, perched estuaries, runup, sediment overwash, Sediment transport, setup, suspension, swash, systems, zone},
	Language = {English},
	Month = mar,
