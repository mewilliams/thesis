@article{peterson_increasing_2002,
	Abstract = {Synthesis of river-monitoring data reveals that the average annual discharge of fresh water from the six largest Eurasian rivers to the Arctic Ocean increased by 7\% from 1936 to 1999. The average annual rate of increase was 2.0 $\pm$ 0.7 cubic kilometers per year. Consequently, average annual discharge from the six rivers is now about 128 cubic kilometers per year greater than it was when routine measurements of discharge began. Discharge was correlated with changes in both the North Atlantic Oscillation and global mean surface air temperature. The observed large-scale change in freshwater flux has potentially important implications for ocean circulation and climate.},
	Author = {Peterson, Bruce J. and Holmes, Robert M. and McClelland, James W. and V{\"o}r{\"o}smarty, Charles J. and Lammers, Richard B. and Shiklomanov, Alexander I. and Shiklomanov, Igor A. and Rahmstorf, Stefan},
