	Keywords = {Nearshore environment, Ocean currents, Ocean waves, Surf zone},
	Month = aug,
	Number = {5-6},
	Pages = {473--481},
	Shorttitle = {Effect of wave directional spread on the radiation stress},
	Title = {Effect of wave directional spread on the radiation stress: comparing theory and observations},
	Url = {http://www.sciencedirect.com/science/article/B6VCX-4D10M91-2/2/7bffd1a56f74385e7c2937aa98ffad83},
