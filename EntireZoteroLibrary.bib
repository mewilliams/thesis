	Keywords = {4240 Oceanography: Sea ice, 4250 Oceanography: Marine geological processes (beaches, turbidity currents, sedimentation)},
	Language = {en},
	Month = jul,
	Number = {C7},
	Pages = {6442--6450},
	Title = {Infragravity energy in the surf zone},
	Url = {http://onlinelibrary.wiley.com/doi/10.1029/JC086iC07p06442/abstract},
	Urldate = {2014-06-19},
	Volume = {86},
