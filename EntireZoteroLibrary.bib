	Keywords = {Air pollution, {GIS} satellite imagery meteorology, Santiago topoclimatology},
	Month = oct,
	Number = {24--25},
	Pages = {4039--4047},
	Title = {Rapid urban growth, land-use changes and air pollution in Santiago, Chile},
	Url = {http://www.sciencedirect.com/science/article/pii/S1352231099001454},
	Urldate = {2013-04-28},
	Volume = {33},
	Year = {1999},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1352231099001454},
