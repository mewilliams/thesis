@article{goodwin_predicting_1996,
	Abstract = {{\textless}p{\textgreater}The losses or degradation of coastal lagoons, tidal wetlands and small estuaries during the past century have been dramatic, exceeding 95\% in some areas of the U.S. There is now widespread concern by regulatory agencies, local governments and public interest groups to preserve remaining tidal wetlands and enhance or restore degraded coastal wetland systems. Many of the characteristics of these ecosystems are governed by the tidal inlet channels which control the tidal exchange, hydroperiod, salinity and water quality, and local flooding. The stability of tidal inlets in many coastal lagoon/tidal wetland systems in California has been altered during the past century due to changes in the watershed, confinement of the inlet channel, and filling or diking of regions of the behind the inlet. The stability of tidal inlets is of ecological concern since periodic closure results in prolonged inundation of foraging habitat; alterations in the salinity characteristics of the wetlands; poorer water quality; and the formation of barriers to fish passage. Therefore, in many saltmarsh enhancement/restoration projects, the stability of the tidal inlet is an important factor. The management objectives of tidal inlets for ecological enhancement differ from the traditional objectives such as navigation, since heavily engineered solutions to stabilize the inlet, or extensive maintenance dredging are usually undesirable. If the level of engineering intervention is to be minimized, it is important to develop an understanding of the natural physical processes at a particular site. A range of semi-empirical criteria for assessing the stability of small-scale natural inlets are reviewed and the sensitivity of these criteria to uncertainties in field data or assumptions in the analyses are discussed.},
	Author = {Goodwin, Peter},
	File = {JSTOR Full Text PDF:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/Q66S4VAT/Goodwin - 1996 - Predicting the Stability of Tidal Inlets for Wetla.pdf:application/pdf},
	Issn = {0749-0208},
	Journal = {Journal of Coastal Research},
	Month = jan,
	Note = {{ArticleType}: research-article / Issue Title: {SPECIAL} {ISSUE} {NO}. 23. Understanding Physical Processes at Tidal Inlets: Based on Contributions by Panel on Scoping Field and Laboratory Investigations in Coastal Inlet Research / Full publication date: {WINTER} 1996 / Copyright {\copyright} 1996 Coastal Education \& Research Foundation, Inc.},
	Pages = {83--101},
	Title = {Predicting the Stability of Tidal Inlets for Wetland and Estuary Management},
	Url = {http://www.jstor.org/stable/25736070},
	Urldate = {2011-08-08},
	Year = {1996},
	Bdsk-Url-1 = {http://www.jstor.org/stable/25736070}}

@article{elwany_opening_1998,
	Abstract = {Over the past 50 yr, direct observations of the inlet status (open or closed) of San Dieguito Lagoon, a typical southern California lagoon located in Del Mar, California, have shown that river flooding is the major natural determinant of inlet conditions on time scales longer than a few years. River flooding is strongly dependent on rainfall in the San Dieguito River watershed and on the influences of two water storage reservoirs in the area. Rainfall fluctuates on yearly and longer time scales and undergoes cycles of wet and dry periods. Over short time periods, ranging from a few months to several years, inlet status is primarily determined by the available tidal prism and littoral sand transport. Recognition of these factors is crucial in order to correctly evaluate the probability that a small lagoon will remain open naturally. A probability approach is essential because the variables controlling inlet conditions are random in nature. The results of our study show that the inlet will remain open naturally 34\% of the time. The tendency to remain open is vastly smaller during years of dry weather (12\%) versus times of above-average rainfall (66\%).},
	Author = {Elwany, M. Hany S. and Flick, Reinhard E. and Aijaz, Saima},
	Issn = {01608347},
