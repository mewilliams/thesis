	Issn = {1814-232X},
	Journal = {Afr. J. Mar. Sci.},
	Keywords = {cirrhosa petagna grande, East Kleinemonde Estuary, estuarine biota, fish assemblages, intermittently open estuary, mouth state, palaemon-peringueyi, permanently open, physical conditions, population-dynamics, recruitment, river flow, salinity regime, upper reaches, warm-temperate, zostera-capensis setchell},
	Language = {English},
