	Month = may,
	Number = {5},
	Pages = {637--649},
	Title = {Axial convergence in a well-mixed estuary},
	Url = {http://www.sciencedirect.com/science/article/B6WDV-4DV0JBT-1C/2/bd7d2444c6a27cdaa7a516e1b168d20d},
	Urldate = {2011-02-18},
