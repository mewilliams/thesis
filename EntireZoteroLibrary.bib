@article{warner_development_2008,
	Abstract = {We are developing a three-dimensional numerical model that implements algorithms for sediment transport and evolution of bottom morphology in the coastal-circulation model Regional Ocean Modeling System ({ROMS} v3.0), and provides a two-way link between {ROMS} and the wave model Simulating Waves in the Nearshore ({SWAN}) via the Model-Coupling Toolkit. The coupled model is applicable for fluvial, estuarine, shelf, and nearshore (surfzone) environments. Three-dimensional radiation-stress terms have been included in the momentum equations, along with effects of a surface wave roller model. The sediment-transport algorithms are implemented for an unlimited number of user-defined non-cohesive sediment classes. Each class has attributes of grain diameter, density, settling velocity, critical stress threshold for erosion, and erodibility constant. Suspended-sediment transport in the water column is computed with the same advection--diffusion algorithm used for all passive tracers and an additional algorithm for vertical settling that is not limited by the {CFL} criterion. Erosion and deposition are based on flux formulations. A multi-level bed framework tracks the distribution of every size class in each layer and stores bulk properties including layer thickness, porosity, and mass, allowing computation of bed morphology and stratigraphy. Also tracked are bed-surface properties including active-layer thickness, ripple geometry, and bed roughness. Bedload transport is calculated for mobile sediment classes in the top layer. Bottom-boundary layer submodels parameterize wave--current interactions that enhance bottom stresses and thereby facilitate sediment transport and increase bottom drag, creating a feedback to the circulation. The model is demonstrated in a series of simple test cases and a realistic application in Massachusetts Bay.},
	Author = {Warner, John C. and Sherwood, Christopher R. and Signell, Richard P. and Harris, Courtney K. and Arango, Hernan G.},
	Doi = {10.1016/j.cageo.2008.02.012},
	Issn = {0098-3004},
	Journal = {Computers \& Geosciences},
	Keywords = {Model coupling, Nearshore modeling, Sediment transport, Three-dimensional numerical model},
	Month = oct,
	Number = {10},
	Pages = {1284--1306},
	Title = {Development of a three-dimensional, regional, coupled wave, current, and sediment-transport model},
	Url = {http://www.sciencedirect.com/science/article/pii/S0098300408000563},
	Urldate = {2013-10-24},
	Volume = {34},
	Year = {2008},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0098300408000563},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.cageo.2008.02.012}}

