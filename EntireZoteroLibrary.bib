	Keywords = {{COAWST} model, Inlet, momentum balance, vortex force, wave-current interaction, Willapa},
	Language = {en},
	Number = {C12},
	Pages = {n/a--n/a},
	Title = {Wave-current interaction in Willapa Bay},
	Url = {http://onlinelibrary.wiley.com/doi/10.1029/2011JC007387/abstract},
