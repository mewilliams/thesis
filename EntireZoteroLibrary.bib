	Keywords = {estuaries, {ESTUARINE} oceanography, Esuaries, Mathematical models, {NUMERICAL} analysis, Numerical analysis/modelling, {OCEAN} circulation, {OCEAN} surface topography, tides},
	Month = mar,
	Number = {3},
	Pages = {548--570},
	Shorttitle = {Drivers of Residual Estuarine Circulation in Tidally Energetic Estuaries},
	Title = {Drivers of Residual Estuarine Circulation in Tidally Energetic Estuaries: Straight and Irrotational Channels with Parabolic Cross Section},
