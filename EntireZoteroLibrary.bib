	Journal = {Coastal Engineering},
	Keywords = {Beaches, Bores, Random waves, Run-up, swash, Wave grouping},
	Month = apr,
	Number = {3},
	Pages = {219--242},
	Title = {Simulation and prediction of swash oscillations on a steep beach},
