	Doi = {10.1016/j.ecss.2010.09.007},
	File = {ScienceDirect Full Text PDF:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/KUKUM82E/Ian T. - 2010 - The hydrodynamics and salinity regime of a coastal.pdf:application/pdf;ScienceDirect Snapshot:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/ITTW3M4W/Ian T. - 2010 - The hydrodynamics and salinity regime of a coastal:},
