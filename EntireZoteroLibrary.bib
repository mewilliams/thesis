	File = {Full Text PDF:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/D9N7438P/Ledwell and Bratkovich - 1995 - A tracer study of mixing in the Santa Cruz Basin.pdf:application/pdf;Snapshot:/Users/megan/Library/Application Support/Firefox/Profiles/eb6pvuzm.default/zotero/storage/8UDWSW3P/full.html:text/html},
	Issn = {2156-2202},
	Journal = {Journal of Geophysical Research: Oceans},
	Language = {en},
	Number = {C10},
	Pages = {20681--20704},
	Title = {A tracer study of mixing in the Santa Cruz Basin},
