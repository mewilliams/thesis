	Pages = {28547--28557},
	Title = {Flow structure in depth-limited, vegetated flow},
	Url = {http://onlinelibrary.wiley.com/doi/10.1029/2000JC900145/abstract},
	Urldate = {2013-09-28},
	Volume = {105},
	Year = {2000},
	Bdsk-Url-1 = {http://onlinelibrary.wiley.com/doi/10.1029/2000JC900145/abstract},
