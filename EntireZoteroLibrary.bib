	Journal = {Continental Shelf Research},
	Keywords = {Oyster farming, Tidal bay infilling, Tidal inlet, Tidal prism},
	Month = jun,
	Number = {9},
	Pages = {1115--1131},
	Shorttitle = {Tidal inlet response to sediment infilling of the associated bay and possible implications of human activities},
	Title = {Tidal inlet response to sediment infilling of the associated bay and possible implications of human activities: the Marennes-Ol{\'e}ron Bay and the Maumusson Inlet, France},
