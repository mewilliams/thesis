@article{gerald_model_2006,
	Abstract = {In a regime of accelerated sea-level rise the Bruun concept predicts that sand will be transferred from the beach to nearshore in order to re-establish the equilibrium slope. Added to this loss of sediment from the barrier is the sand that will be removed from the littoral system at tidal inlets. This paper presents a three-stage conceptual model of sand trapping processes at tidal inlets and the long-term response of the barrier chain to a diminished sediment supply. The model applies to mixed-energy barrier coasts such as those along the East Coast of the United States, East Friesian Islands in the North Sea, and the Copper River Delta barriers in the Gulf of Alaska. These coasts are characterized by short stubby barrier islands, numerous tidal inlets, well developed ebb-tidal deltas, and a backbarrier consisting of supratidal salt marshes, tidal flats, and/or mangroves incised by tidal creeks. Acceleration in the rate of sea-level rise will gradually, or catastrophically depending upon the rate, change the hypsometry of the backbarrier transforming supratidal areas to open water and intertidal environments. The loss of marshlands will increase tidal exchange between the ocean and backbarrier and ultimately change the hydraulic regime of the tidal inlets. The growth of both ebb and flood-tidal deltas diminishes the supply of sand along the coast leading to a fragmentation of the barrier chain and formation of a transgressive coastal system. The ultimate fate of the barriers and their re-establishment onshore is dependent on the trend of sea-level rise. An extensive literature review suggests that backbarrier regions around the world may response to sea-level rise quite differently due to variations in sediment supply and saltmarsh accretion rates.},
	Author = {Gerald, D.M. Fitz and Buynevich, I. and Argow, B.},
	Copyright = {Copyright {\copyright} 2006 Coastal Education \& Research Foundation, Inc.},
	Doi = {10.2307/25741684},
	Issn = {0749-0208},
	Journal = {Journal of Coastal Research},
	Month = jan,
	Note = {{ArticleType}: research-article / Issue Title: Special Issue No. 39. Proceedings of the 8th International Coastal Symposium ({ICS} 2004), Vol. {II} / Full publication date: Winter 2006 / Copyright {\copyright} 2006 Coastal Education \& Research Foundation, Inc.},
	Pages = {789--795},
	Title = {Model of Tidal Inlet and Barrier Island Dynamics in a Regime of Accelerated Sea Level Rise},
	Url = {http://www.jstor.org/stable/25741684},
	Urldate = {2013-09-28},
	Year = {2006},
