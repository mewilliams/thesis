	Keywords = {infragravity, saturation, swash, video imagery, wave dissipation, wave energy fluxes},
	Language = {en},
	Number = {7},
	Pages = {3651--3669},
	Title = {Observations of wave energy fluxes and swash motions on a low-sloping, dissipative beach},
	Url = {http://onlinelibrary.wiley.com/doi/10.1002/jgrc.20267/abstract},
	Urldate = {2013-10-04},
	Volume = {118},
	Year = {2013},
